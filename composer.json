{
  "name": "lobtao/hyperf-helper",
  "description": "quick start -d/reload/restart/stop hyperf(~2.1.0 & ~2.2.0) server",
  "minimum-stability": "stable",
  "keywords": ["php","hyperf","helper"],
  "license": "proprietary",
  "authors": [
    {
      "name": "lobtao",
      "email": "lobtao@gmail.com"
    }
  ],
  "require": {
    "ext-json": "*",
    "ext-posix": "*"
  },
  "require-dev": {
    "swoole/ide-helper": "^4.5"
  },
  "autoload": {
    "psr-4": {
      "lobtao\\helper\\": "src/"
    },
    "files": [
      "./src/functions.php"
    ]
  },
  "extra": {
    "hyperf": {
      "config": "lobtao\\helper\\ConfigProvider"
    }
  }
}
